<?xml version="1.0"?>
<xs:schema xmlns:xs="http://www.w3.org/2001/XMLSchema">
    <xs:element name="solicitud"/>

        <xs:element name="datos"/>
            <xs:element name="nombre" type="xs:string"></xs:element>
            <xs:element name="apellidos" type="xs:string"></xs:element>
            <xs:element name="DNI" type="xs:string"></xs:element>

        <xs:element name="notificaciones"/>
            <xs:element name="domicilio" type="xs:string"></xs:element>
            <xs:element name="CP">
                <xs:simpleType>
                    <xs:restriction base="xs:integer">
                        <xs:totalDigits value="5"></xs:totalDigits>
                    </xs:restriction>
                </xs:simpleType>
            </xs:element>

        <xs:element name="matricula">
            <xs:complexType></xs:complexType>
        </xs:element>
        
</xs:schema>